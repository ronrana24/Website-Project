<%- include('../Includes/header.ejs') %> 

</head>

<body>
    <%- include('../Includes/admin_navigation_bar.ejs') %> 
    <h1>Admin Products</h1>

    <% if (products.length > 0) { %>
        <% products.forEach(function (prod) { %>
            <div class="card col-sm" style="width: 18rem;">
                <img class="card-img-top" src="data:image/png;base64," alt="<%= prod.name %> ">
                <div class="card-body">
                  <h5 class="card-title"><%= prod.name %></h5>
                  <a href="/shop/rana_disposal/edit/<%= prod._id %>">Edit</a>
                  <form action="/shop/rana_disposal/delete/<%= prod._id %>" method="POST">
                    <button class="btn" type="submit">Delete</button>
                  </form>
                </div>
              </div>
        <% }); %> 
    <% } %>

    
<%- include('../Includes/footer.ejs') %>